<%= form_for @skyblog, html: { multipart: true } do |f| %>

<%= f.label :title %>
<%= f.text_field :title %> </br>

<%= f.label :description %>
<%= f.text_field :description %>

<%= f.label :content %>
<%= f.text_field :content %>

<%= f.label :image %>
<%= f.file_field :image %>

<%= f.submit %>
<% end %>